<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE xml>

<?import java.net.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuItem?>

<MenuBar fx:controller="control.view.MenuViewControl" xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml" >
	<menus>
		<Menu text="Fonctions">
			<items>
				<MenuItem fx:id="mensuel" text="Création Vue MEP/TEP" onAction="#switchToMensuel"  />
				<MenuItem fx:id="applications" text="Création Vue par application" onAction="#switchToApplications"  />
			</items>
		</Menu>
		<Menu text="Outils">
			<items>
				<MenuItem fx:id="options" text="Options" onAction="#switchToOptions"  />
				<MenuItem fx:id="planificateur" text="Planificateur" onAction="#switchToPlanificateur"  />
			</items>
		</Menu>
	</menus>

</MenuBar>


